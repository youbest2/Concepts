.PHONY: clean All

All:
	@echo "----------Building project:[ StringsAndPointers - Debug ]----------"
	@cd "StringsAndPointers" && "$(MAKE)" -f  "StringsAndPointers.mk"
clean:
	@echo "----------Cleaning project:[ StringsAndPointers - Debug ]----------"
	@cd "StringsAndPointers" && "$(MAKE)" -f  "StringsAndPointers.mk" clean
